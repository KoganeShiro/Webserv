#   Port and host for each server
#   Server names (optional)
#   Default server for each host:port combination

#   Setup default error pages

#   Specify the maximum allowed client body size

#   List of accepted HTTP methods for each route
#   HTTP redirections
#   Root directory or file path for each route
#   Directory listing option (on/off)
#   Default file for directory requests
#   CGI execution settings based on file extensions
#   POST and GET method handling
#   File upload settings and save locations

#   Full path as PATH_INFO
#   Handling of chunked requests
#   Content-Length handling for CGI output
#   CGI execution directory


# CGI Configuration TO BE CHANGED
# Must be the complete path (and not relative)
cgi {
    bla {
        #cgi_compiler /home/gkubina/Webserv/webserv/testers/ubuntu_cgi_tester;
        cgi_compiler /home/cejin/Desktop/webserv/webserv/testers/ubuntu_cgi_tester;
        cgi_extension bla;
        cgi_timeout 5;
    }

    py {
        #cgi_compiler /home/gkubina/Webserv/webserv/testers/ubuntu_cgi_tester;
        cgi_compiler /home/cejin/Desktop/webserv/webserv/testers/ubuntu_cgi_tester;
        cgi_extension .py;
        cgi_timeout 30;
    }
}

# Server Configuration
server {
    host 170.0.0.1;
    port 8034;
    #server_name# localhost;

    # Error Pages
    #error_page /home/gkubina/Webserv/webserv/html_page/template/error/error.html;
    error_page /home/cejin/Desktop/webserv/webserv/html_page/template/error/error.html;

    #directory_page /home/gkubina/Webserv/webserv/html_page/template/directory_listing/directory.html;
    directory_page /home/cejin/Desktop/webserv/webserv/html_page/template/directory_listing/directory.html;
    
    # Client Body Size Limit
    client_max_body_size 10M;

    # Routes
    location / {
        #root /home/gkubina/Webserv/webserv/html_page/www/html;
        root /home/cejin/Desktop/webserv/webserv/html_page/www/html;
        allowed_methods GET POST DELETE;
        index index.html;
        use_cgi yes;
        autoindex on;
    }

    location /images {
        #root /home/gkubina/Webserv/webserv/html_page/template/error/images;
        root /home/cejin/Desktop/webserv/webserv/html_page/template/error/images;
        index index.html;
        allowed_methods GET;        
    }

    location /success {
        root /home/cejin/Desktop/webserv/webserv/html_page/template/success;
        index index.html;
        allowed_methods GET;        
    }

    location /error {
        root /home/cejin/Desktop/webserv/webserv/html_page/template/error;
        index template.html;
        allowed_methods GET;        
    }

    location /uploads {
        #root /home/gkubina/ebserv/webserv/html_page/www/uploads;
        root /home/cejin/Desktop/webserv/webserv/html_page/www/uploads;
        allowed_methods POST GET DELETE;
        directory_listing yes;
        autoindex on;
    }

    # Redirections TO BE CHANGED
    location /old-page {
        return 301 /new-page;
        allowed_methods GET;
    }
}
